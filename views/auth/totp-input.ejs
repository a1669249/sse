<!DOCTYPE html>
<html>
  <head>
    <title>SSE Voting System</title>
    <!-- Style Sheets -->
    <link rel='stylesheet' href='/stylesheets/style.css'>
    <link rel='stylesheet' href='/libraries/bootstrap/css/bootstrap.min.css'>
    <!-- Libraries -->
    <script src='/libraries/jquery.min.js'></script>
    <script src='/libraries/bootstrap/js/bootstrap.bundle.js'></script>
  </head>
  <body>
    <div class="container-fluid">
      <div id="header" class="row">
        <div class="col">
          <img src="/images/aec-logo-homepage.png">
        </div>
        <div class="text-center m-auto">
        </div>
      </div>
      <div id="content">
        <br>
        <form  action="/auth/login/2fa" method="post">
          <div class="row">
            <div class="col">
              <input type="input" class="form-control text-center" name="code" placeholder="Authenticator Code">
            </div>
          </div>
          <br>
          <div class="row">
            <div class="col text-center m-auto">
              <input type="submit"  class="btn btn-purple" value="Submit"/>
            </div>
          </div>
        </form>
      </div>
      <div id="footer" class="footer">
      </div>
    </div>
  </body>
</html>