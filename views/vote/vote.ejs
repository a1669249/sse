<!DOCTYPE html>
<html>
	<head>
		<title>SSE Voting System</title>
		<!-- Style Sheets -->
		<link rel='stylesheet' href='/stylesheets/style.css'>
		<link rel='stylesheet' href='/libraries/bootstrap/css/bootstrap.min.css'>
		<!-- Libraries -->
		<script src='/libraries/jquery.min.js'></script>
		<script src='/libraries/bootstrap/js/bootstrap.bundle.js'></script>
	</head>
	<body>
		<div class="container-fluid">
			<div id="header" class="row mb-4">
				<div class="col">
					<img src="/images/aec-logo-homepage.png">
				</div>
				<div class="text-center m-auto">
				</div>
				<div class="col text-right m-auto">
					<a class="btn btn-light" href="/auth/logout">Log Out</a>
				</div>
			</div>
			<div id="content">
				<div class="row justify-content-center mb-3">
					<h1 id="electorateName"></h1>
				</div>
				<div class="w-100"></div>
				<div id="aboveLine" class="row mb-2">
				</div>
				<div class="w-100 mb-2" style="height: 2px; background-color: black;"></div>
				<div id="belowLine" class="row mb-3">
				</div>
				<div class="w-100"></div>
				<div class="row justify-content-center">
					<button id="submitBtn" class="btn btn-primary">Submit Vote</button>
				</div>
			</div>
			<div id="footer" class="row"></div>
		</div>
	</body>
	<script src='/javascript/vote.js'></script>
</html>
